{
  "expo": {
    "name": "E-Books",
    "slug": "E-Books",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover"
      },
      "bundleIdentifier": "org.markup.exp"
    },
    "android": {
      "splash": {
        "ldpi": "./assets/logo1.png",
        "mdpi": "./assets/logo2.png",
        "hdpi": "./assets/logo.png",
        "xhdpi": "./assets/logo3.png",
        "xxhdpi": "./assets/logo4.png",
        "xxxhdpi": "./assets/logo5.png",
        "resizeMode": "cover"
      },
      "package": "org.books.exp"
    }
  }
}